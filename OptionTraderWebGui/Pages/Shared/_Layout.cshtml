<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - OptionTraderWebGui</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/OptionTraderWebGui.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
		<nav>
			<div class="container">
				<a asp-area="" asp-page="/Index">OptionTraderWebGui</a>
				<div>
					<ul>
						<li> <a asp-area="" asp-page="/Index">Home</a> </li>
						<li> <a asp-area="" asp-page="/Privacy">Privacy</a> </li>
						<li> <a asp-area="" asp-page="/Connector/Index">Connector</a> </li>
						<li> <a asp-area="" asp-page="/Containers/Index">Containers</a> </li>
					</ul>
				</div>
			</div>
		</nav>
    </header>
    <div class="container">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <footer>
        <div >
            &copy; 2022 - OptionTraderWebGui - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>


    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>